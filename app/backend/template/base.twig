<script type="text/javascript">
    var menuclosed = localStorage.getItem("menustatus");
    if( menuclosed == 'closed'){
        document.querySelector("body").className = "open";
    }

    (function ($) {
        $(document).ready(function() {
            $('#bootstrap-data-table-export').DataTable();
        } );
    })(jQuery);


</script>

